    "# mengambil substring; menggunakan tanda bracket []\n",
    "s = 'TAKUMI'\n",
    "\n",
    "print(s[0])     # menampilkan substring karakter pertama\n",
    "print(s[-4])    #menampilkan substring karakter ke 4  dari kanan\n",
    "print(s[:2])    #menampilkan dua karakter dari awal\n",
    "print(s[:-2])   #menghilangkan dua karakter terakhir\n",
    "print(s[3:])    #menampilkan tiga karakter terakhir\n",
    "print(s[-2:])   #menampilkan dua karakter terakhir \n",
    "print(s[0:3])   #menaampilkan karakter dari 0\n",
    "len(s)    #menunjukan jumlah karakter\n",
    "print(s +' '+ 'oke')  #penggabungan\n",
    "print(s*2)  "
